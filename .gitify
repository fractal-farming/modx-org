data_directory: _data/
data:

    # Contexts
    contexts:
        class: modContext
        primary: key
    context_settings:
        class: modContextSetting
        primary:
            - context_key
            - key
        exclude_keys:
            - editedon
        where:
            - 'key:NOT IN':
                - 'site_url'
                - 'base_url'
                - 'http_host'

    # Basic elements
    categories:
        class: modCategory
        primary: id
        truncate_on_force:
            - modCategoryClosure
    templates:
        class: modTemplate
        primary: templatename
        exclude_keys:
            - static
            - static_file
    template_variables:
        class: modTemplateVar
        primary: name
    template_variables_access:
        class: modTemplateVarTemplate
        primary:
            - tmplvarid
            - templateid
    chunks:
        class: modChunk
        primary: name
        exclude_keys:
            - static
            - static_file

        # Check if static file contains a PatternLab specific path and exclude them if they do.
        # PatternLab elements are updated separately through the installer on live site.
        #where:
        #    'static_file:NOT LIKE': '[[++patternlab.core_path]]%'
    snippets:
        class: modSnippet
        primary: name
        extension: .php
        exclude_keys:
            - static
            - static_file
    plugins:
        class: modPlugin
        primary: name
        extension: .php
        exclude_keys:
            - static
            - static_file
    plugin_events:
        class: modPluginEvent
        primary:
            - pluginid
            - event
    #content:
    #    type: content
    #    exclude_keys:
    #        - editedby
    #        - editedon

    # MODX config
    element_property_set:
        class: modElementPropertySet
        primary:
            - element
            - element_class
            - property_set
    #events:
    #    class: modEvent
    #    primary: name
    extension_packages:
        class: modExtensionPackage
        primary: namespace
        exclude_keys:
            - created_at
            - updated_at
    media_sources:
        class: modMediaSource
        primary: name
    media_sources_elements:
        class: sources.modMediaSourceElement
        primary:
            - source
            - object
            - object_class
            - context_key
    menus:
        class: modMenu
        primary: text
        where:
            'namespace:IN':
                - 'patternlab'
    namespaces:
        class: modNamespace
        primary: name
        where:
            'name:NOT IN':
                - 'gitpackagemanagement'
                - 'patternlab'
    property_set:
        class: modPropertySet
        primary: name
    system_settings:
        class: modSystemSetting
        primary: key
        exclude_keys:
            - editedon
        where:
            'key:NOT IN':
                - 'patternlab.assets_path'
                - 'patternlab.assets_url'
                - 'patternlab.core_path'
                - 'gitpackagemanagement.assets_path'
                - 'gitpackagemanagement.assets_url'
                - 'gitpackagemanagement.core_path'
                - 'gitpackagemanagement.packages_base_url'
                - 'gitpackagemanagement.packages_dir'

    # User config
    access_context:
        class: modAccessContext
        primary: id
    access_media_source:
        class: sources.modAccessMediaSource
        primary: id
    access_policies:
        class: modAccessPolicy
        primary: name
    access_resource_groups:
        class: modAccessResourceGroup
        primary: id
    dashboards:
        class: modDashboard
        primary:
            - id
            - name
    dashboard_widgets:
        class: modDashboardWidget
        primary: id
    dashboard_widget_placement:
        class: modDashboardWidgetPlacement
        primary:
            - dasboard
            - widget
    document_groups:
        class: modResourceGroupResource
        primary: id
    documentgroup_names:
        class: modResourceGroup
        primary: name
    fc_profiles:
        class: modFormCustomizationProfile
        primary: name
    fc_profiles_usergroups:
        class: modFormCustomizationProfileUserGroup
        primary:
            - usergroup
            - profile
    fc_sets:
        class: modFormCustomizationSet
        primary: id
    fc_action_dom:
        class: modActionDom
        primary:
            - set
            - name
    member_groups:
        class: modUserGroupMember
        primary: id
    membergroup_names:
        class: modUserGroup
        primary: name
    usergroup_roles:
        class: modUserGroupRole
        primary: name
    usergroup_settings:
        class: modUserGroupSetting
        primary:
            - group
            - key
        exclude_keys:
          - editedon

    # ContentBlocks
    cb_fields:
        class: cbField
        primary: id
        package: contentblocks
    cb_layouts:
        class: cbLayout
        primary: name
    cb_templates:
        class: cbTemplate
        primary: name
    cb_categories:
        class: cbCategory
        primary: name
    cb_defaults:
        class: cbDefault
        primary: id

    # ClientConfig
    cg_groups:
        class: cgGroup
        primary: label
        package: clientconfig
    cg_settings:
        class: cgSetting
        primary: key

    # Collections
    collections_settings:
        class: CollectionSetting
        primary: collection
        package: collections
    collections_templates:
        class: CollectionTemplate
        primary: name
    collections_template_columns:
        class: CollectionTemplateColumn
        primary: id
    collections_resource_template:
        class: CollectionResourceTemplate
        primary:
            - collection_template
            - resource_template
    collections_selections:
        class: CollectionSelection
        primary:
            - collection
            - resource

    # QuickStartButtons
    qsb_buttons:
        class: qsbButton
        primary: set
        package: quickstartbuttons
    #qsb_icons:
    #    class: qsbIcon
    #    primary: name
    qsb_sets:
        class: qsbSet
        primary: name
    qsb_usergroups:
        class: qsbSetUserGroup
        primary:
            - usergroup
            - set

    # Tagger
    tagger_groups:
        class: TaggerGroup
        primary: name
        package: tagger
    tagger_tags:
        class: TaggerTag
        primary: tag
    tagger_tag_resources:
        class: TaggerTagResource
        primary:
            - tag
            - resource

    # MIGX
    migx_formtab:
        class: migxFormtab
        primary: id
        package: migx
    migx_formtab_field:
        class: migxFormtabField
        primary: id
    migx_config_element:
        class: migxConfigElement
        primary: id
    migx_element:
        class: migxElement
        primary: id
    migx_config:
        class: migxConfig
        primary: id
        exclude_keys:
          - editedby
          - editedon

    # SetInputOptions
    #setinputoptions_groups:
    #    class: SetInputOptionsGroup
    #    primary: name
    #    package: setinputoptions
    #setinputoptions_inputoptions:
    #    class: SetInputOptionsInputOption
    #    primary: id

